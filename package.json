{
  "name": "astro-preact-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "scaffold": "scaffdog generate",
    "createQiitaAPIType": "curl https://qiita.com/api/v2/schema | json2ts > ./src/libs/qiita-types.ts",
    "format": "prettier --write './**/*.{mjs,js,jsx,ts,tsx,astro,json,yml,yaml,md,css,scss,sass}' --cache --ignore-path .gitignore",
    "lint": "eslint './**/*.{mjs,js,jsx,ts,tsx,astro,json}' --fix --cache --ignore-path .gitignore",
    "lint:style": "stylelint './**/*.{css,scss,sass,astro}' --fix --cache --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx,astro}": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ],
    "*.{json,yml,yaml,md,css,scss,sass}": [
      "prettier --write --cache"
    ],
    "*.{css,scss,sass,astro}": [
      "stylelint --fix --cache"
    ]
  },
  "dependencies": {
    "@astrojs/preact": "^1.2.0",
    "@astrojs/sitemap": "^1.0.0",
    "@preact/signals": "^1.1.2",
    "@preact/signals-core": "^1.2.2",
    "astro": "^1.5.0",
    "astro-imagetools": "^0.6.11",
    "astro-seo": "^0.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dayjs": "^1.11.5",
    "destyle.css": "^3.0.2",
    "microcms-js-sdk": "^2.2.1",
    "preact": "^10.11.2",
    "promise-throttle": "^1.1.2",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-hook-form": "^7.37.0",
    "react-imgix": "^9.5.4",
    "satori": "^0.0.42",
    "sharp": "^0.31.1"
  },
  "devDependencies": {
    "@astrojs/image": "^0.10.0",
    "@astrojs/preact": "^1.2.0",
    "@astrojs/prefetch": "^0.0.8",
    "@astrojs/rss": "^1.0.2",
    "@preact/signals": "^1.1.2",
    "@preact/signals-core": "^1.2.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/node": "^18.11.0",
    "@types/react-imgix": "^9.5.0",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "astro": "^1.5.0",
    "astro-compress": "^1.0.12",
    "astro-critters": "^1.0.5",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.20.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "find-up": "^6.3.0",
    "husky": "^8.0.1",
    "json-schema-to-typescript": "^11.0.2",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "preact": "^10.11.2",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.6.0",
    "sass": "^1.55.0",
    "scaffdog": "^2.4.0",
    "stylelint": "^14.14.0",
    "stylelint-config-astro": "^1.0.4",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "volta": {
    "node": "18.9.0",
    "yarn": "1.22.19"
  }
}
