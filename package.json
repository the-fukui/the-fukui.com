{
  "name": "@example/basics",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "yarn typecheck && astro build",
    "preview": "astro preview",
    "createQiitaAPIType": "curl https://qiita.com/api/v2/schema | json2ts > ./src/libs/qiita-types.ts",
    "getPostList": "vite-node ./src/data/getPostList.ts",
    "lint": "eslint ./**/*.{js,ts,tsx,astro,vue} --fix --cache --no-error-on-unmatched-pattern",
    "lint:style": "stylelint --fix ./**/*.{css,scss,sass,astro,vue} --cache",
    "typecheck": "astro check && tsc --noEmit && vue-tsc --noEmit --skipLibCheck --allowJs ./src/**/*.vue ",
    "format": "prettier --write ./**/*.{js,ts,tsx,astro,vue,json,yml,yaml,md} --cache --no-error-on-unmatched-pattern",
    "lint-staged": "lint-staged",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx,astro,vue}": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write --cache"
    ],
    "*.{css,scss,sass,astro,vue}": [
      "stylelint --fix --cache"
    ]
  },
  "devDependencies": {
    "@astrojs/image": "^0.3.1",
    "@astrojs/vue": "^0.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "astro": "^1.0.3",
    "@types/open-graph-scraper": "^4.8.1",
    "dayjs": "^1.11.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.16.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "json-schema-to-typescript": "^11.0.1",
    "lint-staged": "^13.0.3",
    "microcms-js-sdk": "^2.1.0",
    "open-graph-scraper": "^4.11.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.2.0",
    "sass": "^1.54.3",
    "stylelint": "^14.9.1",
    "stylelint-config-astro": "^1.0.4",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typed.css": "brandonmcconnell/typed.css",
    "vite-node": "^0.21.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vue": "^3.2.30",
    "vue-tsc": "^0.39.4"
  },
  "volta": {
    "node": "18.7.0"
  },
  "dependencies": {
    "destyle.css": "^3.0.2"
  }
}
